{
  "name": "zds-shifter-arduino-web",
  "version": "0.0.1",
  "description": "Firmware page from https://zendrumstudio.com/zds-shifter/firmware",
  "author": "Darin Kadrioski",
  "license": "UNLICENSED",
  "main": "app/main.jsx",
  "repository": {
    "type": "git",
    "url": "git@github.com:dkadrios/zds-shifter-arduino-web.git"
  },
  "scripts": {
    "clean": "rimraf build",
    "build:webpack": "NODE_ENV=production webpack -p --config webpack.prod.config.babel.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "open http://localhost:8080/ && webpack-dev-server --config webpack.dev.config.babel.js",
    "lint": "eslint 'src/**' '__tests__/**' '*.js'",
    "lint:fix": "npm run lint -- --fix",
    "lint:msg": "validate-commit-msg",
    "test": "npm run lint",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1"
  },
  "dependencies": {
    "babel-runtime": "6.26.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-markdown": "^3.1.3"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "conventional-changelog-cli": "1.3.5",
    "copy-webpack-plugin": "4.3.0",
    "css-loader": "0.28.7",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "file-loader": "^1.1.6",
    "html-loader": "0.5.1",
    "husky": "0.14.3",
    "less": "2.7.3",
    "less-loader": "4.0.5",
    "markdown-loader": "2.0.1",
    "open-browser-webpack-plugin": "0.0.5",
    "resolve-url-loader": "^2.2.1",
    "style-loader": "0.19.1",
    "validate-commit-msg": "2.14.0",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.9.7"
  }
}
